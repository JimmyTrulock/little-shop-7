<h1>Welcome to the Admin Dashboard</h1>
<p><%= link_to "Admin Merchants", admin_merchants_path%></p>
<p><%= link_to "Admin Invoices", admin_invoices_path%></p>

<h3>Top 5 Chumps</h3>
<% @top5.each_with_index do |customer, index| %>
  <p><%= "#{index+1}. #{customer.first_name} #{customer.last_name} - #{customer.transaction_count} transactions" %></p>
<% end %>

<h3>Items to Ship, Oldest to Newest</h3>
<% @items_to_ship.each do |invoice_item|%>
<section>
  <p><%= link_to "#{invoice_item.invoice_id}", admin_invoice_path(invoice_item.invoice_id) %></p>
</section>
<% end %>